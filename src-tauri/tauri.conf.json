{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../src/dist"
  },
  "package": {
    "productName": "Portable Network Archive",
    "version": "0.0.5"
  },
  "tauri": {
    "macOSPrivateApi": true,
    "updater": {
      "active": true,
      "endpoints": [
        "https://github.com/portable-network-archive/pna-gui/releases/latest/download/latest.json"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEZEMUQ5RjE5MkRBRDkyNUEKUldSYWtxMHRHWjhkL1dkdk9HZHV6VlVibGJrdlNjdXBRRDk2aVRlK0x0OHlrcnpIY3dJV1h5UXMK",
      "windows": {
        "installMode": "passive"
      }
    },
    "allowlist": {
      "all": false,
      "path": {
        "all": true
      },
      "fs": {
        "all": true
      },
      "shell": {
        "all": false,
        "open": true
      },
      "dialog": {
        "all": true
      },
      "window": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.github.portable-network-archive.gui",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "transparent": true,
        "fullscreen": false,
        "resizable": true,
        "title": "portable-network-archive",
        "width": 950,
        "height": 450
      }
    ]
  }
}
